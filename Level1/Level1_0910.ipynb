   "source": [
    "# 배열 x 배수 증가\n",
    "\n",
    "def solution(x, n):\n",
    "    answer = []\n",
    "    \n",
    "    for i in range(1, n+1):\n",
    "        answer.append(x*i)\n",
    "    \n",
    "    return answer"
   ]
  }
